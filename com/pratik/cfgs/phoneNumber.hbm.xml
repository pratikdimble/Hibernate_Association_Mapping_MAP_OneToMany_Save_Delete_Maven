  create table user_table(user_id number(5) primary key,
                                            first_name varchar2(20));
                                            
                                            
  create table Phone_numbers(phone number(10) primary key,
                                                      number_type varchar2(20),
                                                      unid number(5) references user_table(user_id)); 
                                                                                                
 -->